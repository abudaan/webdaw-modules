{
  "name": "webdaw-examples",
  "version": "1.0.0",
  "description": "some examples using WebDAW modules",
  "main": "index.js",
  "dependencies": {
    "@types/ramda": "^0.27.32",
    "@types/webmidi": "^2.0.4",
    "opensheetmusicdisplay": "^1.5.3",
    "ramda": "^0.27.1",
    "rxjs": "^6.6.3",
    "zustand": "^3.2.0",
    "process": "^0.11.10",
    "webdaw-modules": "../"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "css-modules-typescript-loader": "^4.0.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "file-loader": "^6.2.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.3.1",
    "to-string-loader": "^1.1.6",
    "ts-loader": "^8.0.12",
    "typed-css-modules-loader": "0.0.18",
    "typescript": "^4.1.3",
    "webpack": "^5.10.1",
    "webpack-cli": "^4.2.0"
  },
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "watch": "webpack --watch --config ./webpack.config.js",
    "dev1": "webpack-dev-server --config ./webpack.config.js --display-error-details",
    "server": "cd ../ && python3 -m http.server 9000",
    "dev": "npm run server & npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abudaan/webdaw-modules.git"
  },
  "keywords": [
    "MIDI",
    "audio",
    "web",
    "MPE"
  ],
  "author": "abudaan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abudaan/webdaw-modules/issues"
  },
  "homepage": "https://github.com/abudaan/webdaw-modules#readme",
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "overrides": [
      {
        "files": ".scss",
        "options": {
          "parser": "scss"
        }
      }
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "globals": {},
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-dupe-class-members": "off",
      "@typescript-eslint/no-unused-vars": "warn",
      "@typescript-eslint/no-explicit-any": "warn",
      "@typescript-eslint/no-inferrable-types": "off",
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/interface-name-prefix": "off"
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "prettier",
      "react/recommended"
    ]
  }
}
